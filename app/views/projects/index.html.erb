<table class="table table-striped">
  <thead>
    <tr>
      <th>Titre</th>
      <th>Url</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= link_to(project.title, project_path(project)) %></td>
        <td><%= link_to(project.url, project_path(project)) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
